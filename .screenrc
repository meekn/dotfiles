defutf8 on
defencoding utf8
encoding utf-8 utf-8

defflow off

escape ^z^z

bind x
bind ^x

startup_message off
autodetach on
vbell off
altscreen on

caption always "%-w%10L>%{=b bw}%n %t%{-}%+w%-0="
hardstatus alwayslastline "%H%=%Y/%m/%d %02c"
shelltitle "$ |shell"

bindkey -k kD stuff \177

termcapinfo xterm* ti@:te@

shell $SHELL
term screen-256color
